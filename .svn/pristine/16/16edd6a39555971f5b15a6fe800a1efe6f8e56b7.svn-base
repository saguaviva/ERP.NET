    Private Sub btnGenerarRecibo_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnGenerarComisiones.Click
        Dim totalIVA, totalIRPF As Double
        Try
            registroActual.generarComisiones(nz(dtpDeFecha.Value, Date.Now, False, True), nz(dtpAFecha.Value, Date.Now, False, True), nz(txtIVA.Value, 0, True), nz(txtIRPF.Value, 0, True))
            registroActual.dtComisiones.DefaultView.Sort = "FRA DESC"
            dgConsumos.SetDataBinding(registroActual.dtComisiones.DefaultView, "")
            IniciarDGConsumos()
            txtTotalBase.Value = Format(nzn(sumaTotal("BASE1", registroActual.dtComisiones.DefaultView), 0), "#,##0.00")
            txtTotalComis.Value = Format(nzn(sumaTotal("TOTALCOMIS", registroActual.dtComisiones.DefaultView), 0), "#,##0.00")
            totalIVA = txtTotalBase.Value * txtIVA.Value
            totalIRPF = txtTotalBase.Value * txtIRPF.Value
            txtTotalComisionImpuestos.Value = txtTotalBase.Value - totalIRPF + totalIVA

        Catch ex As Exception
            LOG(ex.ToString)
        End Try

    End Sub

    Private Sub btnExportarExcel_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnExportarExcel.Click
        Try
            Dim xls As New ExcelDirecto.ExcelDirecto
            Dim l As New ListBox
            Dim i As Integer
            For i = 0 To dgConsumos.Splits(0).DisplayColumns.Count - 1
                l.Items.Add(dgConsumos.Splits(0).DisplayColumns(i).DataColumn.DataField)
            Next
            Dim clsa As New clsArrays
            xls.CrearExcel(dgConsumos, "Comisions REPRESENTANT", True)

        Catch ex As Exception
            LOG(ex.ToString)
        End Try

    End Sub

