                cargando = True
                If chkANULADA.Checked = True Then
                    chkRecibido.Checked = False
                    lblFechaRecibido.Visible = False
                End If
                cargando = False
            End If
        Catch ex As Exception
            LOG(ex.ToString) : CCN() : cargando = False
        End Try
