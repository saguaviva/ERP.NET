    Private Sub cboID_ItemChanged(ByVal sender As Object, ByVal e As System.EventArgs) Handles cboid.RowChange
        Try
            If consulta() Then
                cargando = True
                operacionActual.CambiarAReg(nz(cboID.WillChangeToValue, ""), iraregistro)
                PSBTIPO(operacionActual.centro) : cargando = False
            End If
        Catch ex As Exception
