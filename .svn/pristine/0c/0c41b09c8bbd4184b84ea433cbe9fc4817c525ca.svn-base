        Dim puerto As Integer
        Dim bbdd As String
        Try

            xmlDoc.Load(CurDir() & "\conf\configuracion.xml")
            For Each tempnode In xmlDoc.DocumentElement.ChildNodes
                Select Case tempnode.Name
                    Case "IPSERVIDOR"
                        host = tempnode.InnerText
