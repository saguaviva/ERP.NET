#Region "LEERCONFIGINICIALXML"

    Private Sub PonerIdiomaEmpresa()
        Dim xmlDoc As New Xml.XmlDocument
        Dim tempnode As Xml.XmlNode
        Dim lang As String
        Try
            xmlDoc.Load(CurDir() & "\conf\configuracion.xml")
            For Each tempnode In xmlDoc.DocumentElement.ChildNodes
                Select Case tempnode.Name
                    Case "IDIOMA"
                        lang = tempnode.InnerText
                    Case "EMPRESADEFECTO"
                        empresaPorDefecto = general.nz(tempnode.InnerText, "C")
                    Case "IDIOMAIMPRESION"
                        idiomaImpresion = tempnode.InnerText
                End Select
            Next
            System.Threading.Thread.CurrentThread.CurrentUICulture = New System.Globalization.CultureInfo(lang)

        Catch ex As Exception
            LOG(ex.ToString)
        End Try
    End Sub

