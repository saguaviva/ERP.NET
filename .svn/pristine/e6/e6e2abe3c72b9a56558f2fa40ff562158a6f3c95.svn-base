    Private Sub ToolBar1_ButtonClick1(ByVal sender As System.Object, ByVal e As TD.SandBar.ToolBarItemEventArgs) Handles ToolBar1.ButtonClick
        Try
            Dim f As Form
            Dim b As TD.SandBar.ButtonItem
            For Each b In ToolBar1.Items
                b.Checked = False
            Next
            For Each f In Me.MdiChildren
                If f.Text = e.Item.ToolTipText Then
                    f.BringToFront()
                    f.Show()
                    CType(e.Item, TD.SandBar.ButtonItem).Checked = True
                    Return
                End If
            Next
        Catch ex As Exception
            LOG(ex.ToString) : cargando = False : CCN()
        End Try
    End Sub

