    Private Sub IniciarForm(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles MyBase.Load
        Cursor = Cursors.AppStarting
        Dim xmlDoc As New Xml.XmlDocument
        Dim tempnode As Xml.XmlNode
        Dim host As String
        Dim puerto As Integer
        Try

            xmlDoc.Load(directorio & "conf\configuracion.xml")
            For Each tempnode In xmlDoc.DocumentElement.ChildNodes
                Select Case tempnode.Name
                    Case "IPSERVIDOR"
                        host = tempnode.InnerText
                    Case "PUERTO"
                        puerto = tempnode.InnerText
                    Case "MODODEBUG"
                        Try
                            modoDebug = tempnode.InnerText
                        Catch ex As Exception : End Try
                End Select
            Next

            PonerChecksIdioma()

            If modoDebug Then
                monitor = New MySqlMonitor
                monitor.IsActive = True
            End If
            mHost = host
            cnn = New MySqlConnection("User=root;Password=J7V21R54;Host=" & host & " ;Database=coespunt;Port=" & puerto)

            PonerImagenFondo()

            mnuBackup.Visible = modoDebug
            mnuErrores.Visible = modoDebug
            mnuUtilidades.Visible = modoDebug
            mnuBackup.Visible = modoDebug
            mnuStockModelos.Visible = modoDebug
            mnuEnviarErrores.Visible = modoDebug

            Try
                cnn.Open()
                Dim cmdSelect As New MySqlCommand
                Dim daSelect As New MySqlDataAdapter
                cmdSelect.Connection = cnn
                cmdSelect.CommandText = "SELECT CODI, DESCRI, DESCRI AS NOMFILIAL FROM FILIALES"
                daSelect.SelectCommand = cmdSelect
                ACN()
                dtFiliales.Rows.Clear()
                daSelect.Fill(dtFiliales)
                PonerCaptions(dtFiliales)
                dtFiliales.AcceptChanges()
                cnn.Close()

            Catch ex As Exception
                MessageBox.Show(rm.GetString("ERRORCN"))
                CargaFormulario(frmConfiguracion.GetInstance)
                'salirSinConfirmar = True
                'Me.Close()
            End Try
            ComprobarLOG()
            'CargaFormulario(frmEstFacturacionArticulos.GetInstance)

        Catch ex As Exception
            LOG(ex.ToString) : cargando = False : CCN()
        End Try
        Cursor = Cursors.Default
    End Sub
    Private Sub PonerChecksIdioma()
        Try
            'Idioma del programa
            Select Case System.Threading.Thread.CurrentThread.CurrentUICulture.Name
                Case "ca"
                    mnuCatalan.Checked = True
                    mnuCastellano.Checked = False
                Case "es-ES"
                    mnuCatalan.Checked = False
                    mnuCastellano.Checked = True
            End Select

            'Idioma de impresion
            Select Case idiomaImpresion
                Case "ca"
                    mnuCatalanImpresion.Checked = True
                    mnuCastellanoImpresion.Checked = False
                    mnuInglesImpresion.Checked = False
                Case "es-ES"
                    mnuCatalanImpresion.Checked = False
                    mnuCastellanoImpresion.Checked = True
                    mnuInglesImpresion.Checked = False
                Case "en"
                    mnuCatalanImpresion.Checked = False
                    mnuCastellanoImpresion.Checked = False
                    mnuInglesImpresion.Checked = True
            End Select

        Catch ex As Exception
            LOG(ex.ToString)
        End Try
    End Sub
