#Region "IDIOMAS"


    Private Sub GuardarIdioma(ByVal idioma As String, Optional ByVal impresion As Boolean = False)
        Try
            Dim xmlWri As Xml.XmlWriter
            Dim lang As String


            ' Create an Xml document instance and load XML data.
            Dim xmlDoc As Xml.XmlDocument = New Xml.XmlDocument
            xmlDoc.Load(directorio & "conf\configuracion.xml")

            ' I. Modification
            ' 1. Increment all the Book Id attribute values by 100.
            Dim tempnodeList As Xml.XmlNodeList
            If impresion Then
                tempnodeList = xmlDoc.SelectNodes("//IDIOMAIMPRESION")
                idiomaImpresion = idioma
            Else
                tempnodeList = xmlDoc.SelectNodes("//IDIOMA")
            End If

            Dim tempnode As Xml.XmlNode
            For Each tempnode In tempnodeList
                tempnode.InnerText = idioma
            Next
            ' 2. Save the modified XML to a file in Unicode format.
            xmlDoc.PreserveWhitespace = True
            Dim wrtr As Xml.XmlTextWriter = New Xml.XmlTextWriter(directorio & "conf\configuracion.xml", System.Text.Encoding.Unicode)
            xmlDoc.WriteTo(wrtr)
            wrtr.Close()
            w.Close()
            If Not impresion Then
                Application.Exit()
                System.Diagnostics.Process.Start(Application.ExecutablePath)
            End If

        Catch ex As Exception
            LOG(ex.ToString)
        End Try
    End Sub
    Private Sub mnuCatalanImpresion_Activate(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnuCatalanImpresion.Activate
        Try
            System.Threading.Thread.CurrentThread.CurrentUICulture = New CultureInfo("ca")
            mnuCatalanImpresion.Checked = True
            mnuInglesImpresion.Checked = False
            mnuCastellanoImpresion.Checked = False
            GuardarIdioma("ca", True)
            'PonerNombresIdioma()

        Catch ex As ArgumentException
            LOG(ex.Message.ToString)
        End Try
    End Sub

    Private Sub mnuCastellanoImpresion_Activate(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles mnuCastellanoImpresion.Activate
        Try
