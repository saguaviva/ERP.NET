    Private Sub CargaFormulario(ByVal f As Form)
        Try
            Cursor = Cursors.WaitCursor
            CargandoFormulario()
            f.MdiParent = Me
            AddHandler f.Closed, AddressOf childCerrado
            AddHandler f.Load, AddressOf childAbierto
            AddHandler f.Activated, AddressOf childOcultandoMostrando
            f.Show()
            f.BringToFront()
            Cursor = Cursors.Default
            fcargando.Close()

        Catch ex As Exception
            LOG(ex.ToString) : cargando = False : CCN() : fcargando.Close()
        End Try
    End Sub
    Public Sub childOcultandoMostrando(ByVal sender As Object, ByVal e As System.EventArgs)
        Dim f As Form
        Dim i As Integer
        Dim b As TD.SandBar.ButtonItem
        Try
            If sender.Visible = False Then
                For i = 0 To ToolBar1.Items.Count - 1
                    If ToolBar1.Items(i).ToolTipText = sender.TEXT Then
                        DirectCast(ToolBar1.Items(i), TD.SandBar.ButtonItem).Checked = False
                    End If
                Next
            Else
                For i = 0 To ToolBar1.Items.Count - 1
                    If ToolBar1.Items(i).ToolTipText = sender.TEXT Then
                        DirectCast(ToolBar1.Items(i), TD.SandBar.ButtonItem).Checked = True
                    Else
                        DirectCast(ToolBar1.Items(i), TD.SandBar.ButtonItem).Checked = False
                    End If
                Next
            End If

        Catch ex As Exception
            LOG(ex.ToString) : cargando = False : CCN()
        End Try
    End Sub
    Public Sub childCerrado(ByVal sender As Object, ByVal e As System.EventArgs)
        Dim ya As Boolean = True
        Dim f As Form
        Dim b As TD.SandBar.ButtonItem
        Try
            ToolBar1.Items.Clear()
            For Each f In Me.MdiChildren
                b = New TD.SandBar.ButtonItem
                b.Text = f.Text
                b.ToolTipText = f.Text
                If Not sender.Text = f.Text Then
                    If ya Then
                        b.Checked = True
                        f.BringToFront()
                    End If
                    b.BeginGroup = True
                    b.ImageIndex = 22
                    ToolBar1.Items.Add(b)
                    ya = False
                End If
            Next

        Catch ex As Exception
            LOG(ex.ToString) : cargando = False : CCN()
        End Try
    End Sub
