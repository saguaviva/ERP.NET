    Private Sub timActualizador_Tick(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles timActualizador.Tick
        Dim sFileInfo As IO.FileInfo
        Dim dFileInfo As IO.FileInfo
        Dim sFile, sourceDir, destDir As String
        Try
            sFile = "tex.net.exe"

            If sFileInfo.LastWriteTime > dFileInfo.LastWriteTime Then
                sFileInfo = New System.IO.FileInfo(sFile)
                dFileInfo = New System.IO.FileInfo(Replace(sFile, sourceDir, destDir))
            End If

        Catch ex As Exception
            LOG(ex.ToString)
        End Try
    End Sub
