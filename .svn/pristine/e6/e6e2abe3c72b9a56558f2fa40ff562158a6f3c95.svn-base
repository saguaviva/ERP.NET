    Private Sub PonerImagenFondo()
        Try
            Dim xControl As Object
            Dim imgDest As New Bitmap(Me.ClientSize.Width, Me.ClientSize.Height)
            Dim xGraphics As Graphics = Graphics.FromImage(imgDest)

            xGraphics.DrawImage(OriginalImage, 0, 0, imgDest.Width + 1, imgDest.Height + 1)

            For Each xControl In Me.Controls
                If xControl.GetType.ToString = "System.Windows.Forms.MdiClient" Then
                    xMDIClient = xControl
                    xControl.BackgroundImage = imgDest
                    'directcast(xControl, System.Windows.Forms.MdiClient).BackgroundImage.PhysicalDimension
                    Exit For
                End If
            Next

            xGraphics.Dispose()

        Catch ex As Exception
            LOG(ex.ToString) : cargando = False : CCN()
        End Try
    End Sub
    Private Sub xMDIClient_Resize(ByVal sender As Object, ByVal e As System.EventArgs) Handles xMDIClient.Resize
        DrawBG()
    End Sub
    Private Sub DrawBG()
        Try
            If Me.WindowState <> FormWindowState.Minimized Then
                Dim imgDest As New Bitmap(Me.ClientSize.Width, Me.ClientSize.Height)
                Dim xGraphics As Graphics = Graphics.FromImage(imgDest)
                xGraphics.DrawImage(OriginalImage, 0, 0, imgDest.Width + 1, imgDest.Height + 1)
                xMDIClient.BackgroundImage = imgDest
                xGraphics.Dispose()
            End If

        Catch ex As Exception
            LOG(ex.ToString)
        End Try
    End Sub

