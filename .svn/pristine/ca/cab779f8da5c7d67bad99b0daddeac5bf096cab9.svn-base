            aDomicilio(2) = "string"
            aDomicilio(3) = 3

            aTelefono(0) = "TEL"
            aTelefono(1) = "Telefon"
            aTelefono(2) = "string"

            aFax(0) = "FAX"
            aFax(1) = "FAX"
            aFax(2) = "string"

            aCP(0) = "CP"
            aCP(1) = "C.P."
            aCP(2) = "integer"

            aPoblacion(0) = "POB"
            aPoblacion(1) = "Població"
            aPoblacion(2) = "string"

            aProvincia(0) = "PROV"
            aProvincia(1) = "Provincia"
            aProvincia(2) = "string"
            aNIF(0) = "NIF"
            aNIF(1) = "NIF"
            aNIF(2) = "string"

            aContacto(0) = "CONTACTE"
            aContacto(1) = "Contacte"
            aContacto(2) = "string"

            aForma(0) = "FORMA"
            aForma(1) = "Forma pag"
            aForma(2) = "string"

            aIVA(0) = "IVA"
            aIVA(1) = "IVA"
            aIVA(2) = "string"

            'Faltan banco y repres pq solo pone el codigo solucion???
            With arrayCampos
                .Add(CCTallers, aCodigo)
                .Add(CNTallers, aNombre)
                .Add("DOM", aDomicilio)
                .Add("CP", aCP)
                .Add("TEL", aTelefono)
                .Add("FAX", aFax)
                .Add("POB", aPoblacion)
                .Add("PROV", aProvincia)
                .Add("NIF", aNIF)
                .Add("CONTACTE", aContacto)
                .Add("IVA", aIVA)
                .Add("FORMA", aForma)

                Dim aNOMCENTRO(3) As String
