    Public Function ObtenerArrayCamposTalleres() As Hashtable
        Try
            Dim arrayCampos As New Hashtable
            Dim aCodigo(3) As String
            aCodigo(0) = "CODI"
            aCodigo(1) = "Codi"
            aCodigo(2) = "integer"
