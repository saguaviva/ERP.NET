    Private Sub btnElegirTaller_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnElegirTaller.Click
        Try
            If PuedoModificar() Then
                cargando = True
                Cursor = Cursors.WaitCursor
