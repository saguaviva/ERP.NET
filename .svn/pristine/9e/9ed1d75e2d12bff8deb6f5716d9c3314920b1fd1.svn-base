        Dim SQL As String
        Try
            If consulta() Then
                cargando = True
                tallerActual.CambiarAReg(ponercontrabarrasreal(cboid.WillChangeToValue), iraregistro)
                PSBTIPO(tallerActual.centro) : cargando = False
