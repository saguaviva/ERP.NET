    Private Sub IniciarForm(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles MyBase.Load
        Cursor = Cursors.WaitCursor
        Try
            cargando = True
            Me.arrayTextBox.Controls.AddRange(New System.Windows.Forms.Control() {Me.txtTEL2, Me.txtCP, Me.txtDOM, Me.txtEMAIL1, Me.txtEMAIL2, Me.txtFAX, Me.txtFORMAT, Me.txtNIF, Me.txtOBSERV, Me.txtPOB, Me.txtPROV, Me.txtTEL, Me.txtWEB, Me.tx2CODI, Me.tx3CODI})
            Me.arrayGrids.Controls.AddRange(New System.Windows.Forms.Control() {Me.dgAcabados})
            Me.arrayTabPages.Controls.AddRange(New System.Windows.Forms.Control() {Me.TabPage2, Me.tbTaller, Me.tpAcabadosProveedor})
            Me.arrayBotones.Controls.AddRange(New System.Windows.Forms.Control() {Me.btnElegirTaller})
            Me.arrayCombos.Controls.AddRange(New System.Windows.Forms.Control() {Me.cb2NOM, Me.cb3NOM})
            ACN()
            tabla = tablaTalleres
            tallerActual = New clsTaller(ds.Tables(tablaTalleres), "C", BindingContext) 'nz(cboSeleccionCentro.SelectedValue, empresaPorDefecto), BindingContext)
            HacerBindings()
            inicializarDgs()
            PonerModificables(soloLectura)
            CCN()
            PonerHandlersErroresParaGrids()
            PSBTIPO(tallerActual.centro) : cargando = False

        Catch ex As Exception
            LOG(ex.ToString) : cargando = False : CCN()
        End Try

        Cursor = Cursors.Default
    End Sub
    Private Sub HacerBindings()
        Try
            Me.txtCODI.DataBindings.Add(New System.Windows.Forms.Binding("Text", tallerActual.dvForm, "CODI"))
            cboid.DataBindings.Add(New System.Windows.Forms.Binding("Text", tallerActual.dvForm, "NOM"))

            AñadirBindingCombo(cboid, tallerActual.dvIdentificadores, CCTallers, CNTallers)
            OcultarMostrarColumnaCbo(cboid, "CENTRO", False)

            dgAcabados.SetDataBinding(tallerActual.acabados.dvForm, "")
            HacerBindingsTodos(tallerActual.dvForm)

        Catch ex As Exception
            LOG(ex.ToString) : cargando = False : CCN()
        End Try

    End Sub
    Private Sub inicializarDgs()
        Dim i As Integer
        Try
            OcultarColumnasDG(dgAcabados)
            i = 0

            i = 1
            PPCol2("CODI", dgAcabados, rm.GetString("CODIGO"), "", True, 200, _
                        False, PresentationEnum.Normal, False, 200, i, False)

            i = i + 1
            PPCol2("PREUM", dgAcabados, rm.GetString("PREUM"), "", True, 80, _
                        False, PresentationEnum.Normal, False, 80, i, False)

            i = i + 1
            PPCol2("PREUK", dgAcabados, rm.GetString("PREUK"), "", True, 80, _
                        False, PresentationEnum.Normal, False, 80, i, False)


        Catch ex As Exception
            LOG(ex.ToString) : cargando = False : CCN()
        End Try
    End Sub

