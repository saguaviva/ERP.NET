    Private Function obtenerComposicion() As String
        Dim i As Integer
        Dim s As String
        Try

            For i = 0 To tejidoActual.composicion.dvForm.Count - 1

                s = s & " " & tejidoActual.composicion.dvForm(i).Item("PER") & "% " & tejidoActual.composicion.dvForm(i).Item("COMP")
            Next
            Return s
